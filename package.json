{
  "name": "nodejs-prisma-example",
  "version": "0.0.1",
  "description": "NodeJS Prisma Example",
  "main": "server.ts",
  "scripts": {
    "serve": "cross-env NODE_ENV=local.development nodemon",
    "build": "tsc && tsc-alias",
    "test": "cross-env NODE_ENV=test jest --coverage",
    "test:silent": "cross-env NODE_ENV=test jest --coverage --silent",
    "lint:check": "npx eslint src/**/*.ts",
    "lint:fix": "npx eslint --fix src/**/*.ts",
    "prisma:update": "npx prisma db push",
    "prisma:migrate": "npx prisma migrate dev",
    "prisma:migrate:prod": "npx prisma migrate deploy",
    "prisma:studio": "npx prisma studio"
  },
  "author": "Thomas Cybulski",
  "license": "MIT",
  "engines": {
    "node": ">= 16.13.0"
  },
  "devDependencies": {
    "@jest-mock/express": "^1.4.5",
    "@types/bcryptjs": "^2.4.2",
    "@types/compression": "^1.7.2",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/hpp": "^0.2.2",
    "@types/jest": "^27.4.1",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/morgan": "^1.9.3",
    "@types/node": "^17.0.23",
    "@types/supertest": "^2.0.12",
    "@types/swagger-jsdoc": "^6.0.1",
    "@types/swagger-ui-express": "^4.1.3",
    "@types/ws": "^8.5.3",
    "@typescript-eslint/eslint-plugin": "^5.16.0",
    "@typescript-eslint/parser": "^5.16.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.11.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-import-resolver-typescript": "^2.7.0",
    "eslint-plugin-import": "^2.25.4",
    "jest": "^27.5.1",
    "jest-mock-extended": "^2.0.4",
    "nodemon": "^2.0.15",
    "prettier": "^2.6.0",
    "prisma": "^4.1.0",
    "supertest": "^6.2.4",
    "ts-jest": "^27.1.3",
    "ts-node": "^10.7.0",
    "tsc-alias": "^1.6.4",
